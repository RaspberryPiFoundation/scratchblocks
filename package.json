{
  "name": "scratchblocks",
  "version": "3.1.2",
  "description": "Render scratchblocks code to SVG images.",
  "browser": "browser.js",
  "main": "bundle.js",
  "directories": {
    "test": "tests"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "browserify browser.js -o build/scratchblocks.min.js -t [ babelify --presets [ es2015 babili ] ]",
    "fmt": "prettier --write *.js syntax/*.js scratch2/*.js",
    "start": "beefy browser.js:bundle.js --live --port 8000",
    "postinstall": "npx babel browser.js --out-file bundle.js --source-maps",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjvr/scratchblocks.git"
  },
  "keywords": [
    "scratch"
  ],
  "author": "tjvr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjvr/scratchblocks/issues"
  },
  "homepage": "https://scratchblocks.github.io/",
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "babili": "^0.0.11",
    "beefy": "^2.1.8",
    "browserify": "^14.1.0"
  },
  "devDependencies": {
    "jest": "^19.0.2",
    "prettier": "1.7.4"
  }
}
